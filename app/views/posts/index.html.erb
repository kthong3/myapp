<% @posts.each do |post| %>
  <div class="post_wrapper">
    <h2 class="title"><%= link_to post.title, post %></h2>
    <p class="date"><%= post.created_at.strftime("%B, %d, %Y") %>. Submitted by <%= post.author.full_name %></p>
    <p>
      <% post.tags.each do |tag| %>
        <%= tag.name %>
      <% end %>
    </p>
  </div>
<% end %>
